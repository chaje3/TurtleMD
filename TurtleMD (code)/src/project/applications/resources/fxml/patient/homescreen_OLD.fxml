<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.applications.controllers.patient.HomeController">
    <children>
        <Button fx:id="NextAppointmentBtn" mnemonicParsing="false" onMouseReleased="#NextAppointmentBtnAction" prefHeight="50.0" prefWidth="125.0" text="Next Appointment">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
        <Button fx:id="viewScheduleBtn" layoutX="125.0" mnemonicParsing="false" onMouseReleased="#viewScheduleBtnAction" prefHeight="50.0" prefWidth="125.0" text="View Schedule">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
        <Button fx:id="editProfileBtn" layoutX="250.0" mnemonicParsing="false" onMouseReleased="#editProfileAction" prefHeight="50.0" prefWidth="125.0" text="Edit Profile">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
        <Button fx:id="signOutBtn" layoutX="375.0" mnemonicParsing="false" onMouseReleased="#signOutAction" prefHeight="50.0" prefWidth="125.0" text="Sign Out">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
