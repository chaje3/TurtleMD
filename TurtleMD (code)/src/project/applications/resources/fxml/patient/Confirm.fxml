<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="211.0" prefWidth="386.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.applications.controllers.patient.ConfirmController">
   <children>
      <Label alignment="CENTER" layoutX="15.0" layoutY="14.0" prefHeight="97.0" prefWidth="357.0" text="Are these symptoms correct?">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="yesBtn" layoutX="54.0" layoutY="125.0" mnemonicParsing="false" onMouseReleased="#yesBtnAction" prefHeight="36.0" prefWidth="104.0" text="Yes" />
      <Button fx:id="noBtn" layoutX="236.0" layoutY="125.0" mnemonicParsing="false" onMouseReleased="#noBtnAction" prefHeight="36.0" prefWidth="104.0" text="No" />
   </children>
</AnchorPane>
